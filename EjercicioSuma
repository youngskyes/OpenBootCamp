public class SumaNumeros {
    public static void main(String[] args) {
        // funciÃ³n sumar
        int resultado = sumar(21, 1, 81);
        System.out.println(resultado);
    }

    public static int sumar(int num1, int num2, int num3) {
        int suma = num1 + num2 + num3;
        return suma;
    }
}
